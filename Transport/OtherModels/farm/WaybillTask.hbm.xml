<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Transport" namespace="Transport.OtherModels.farm">

  <class name="WaybillTask" lazy="false">

    <id name="TaskId">
      <generator class="native"/>
    </id>

    <many-to-one class="Waybill" name="Waybill" column="WaybillId"/>
    <property name="CustomerId" />
    <property name="VehicleNormId" />
    <property name="TaskDate" />
    <property name="FuelId" />
    <property name="WorkAmount" />
    <property name="WeightKm" />
    <property name="Weight" />
    <property name="isUnaccounted" />

    <property name="CostCode" />
    
    <bag name="WaybillTaskNormIncreases" lazy="false" fetch="join" outer-join="true">
      <key>
        <column name="WaybillId" unique-key="key_TaskIncrease"/>
      </key>
      <element>
        <column name="IncreaseId" unique-key="key_TaskIncrease"/>
      </element>
    
  
  </bag>
    
  </class>

</hibernate-mapping>