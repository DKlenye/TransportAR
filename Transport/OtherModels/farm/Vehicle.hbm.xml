<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Transport" namespace="Transport.OtherModels.farm" default-lazy="false">


  <class name="BaseVehicle" table="Vehicle">

    <id name="VehicleId" >
      <generator class="native"/>
    </id>
    
    <property name="OwnerId" />
    <property name="Model" />

    <property name="GarageNumber" />
    <property name="RegistrationNumber" />
    <property name="InventoryNumber" />


    <property name="StartWork" />
    <property name="EndWork" />
    <property name="WaybillTypeId"/>
    <property name="CustomerId"/>

  </class>




  <class name="Vehicle">

    <id name="VehicleId" >
      <generator class="native"/>
    </id>

    <property name="OwnerId" />
    <property name="Model" />

    <property name="GarageNumber" />
    <property name="RegistrationNumber" />
    <property name="InventoryNumber" />  
    
    
    
    <property name="StartWork" />
    <property name="EndWork" />

    <property name="WaybillTypeId"/>
    <property name="CustomerId"/>

    <bag name="VehicleDriver" >
      <key>
        <column name="VehicleId"/>
      </key>
      <one-to-many class="VehicleDriver"/>
    </bag>

    <bag name="VehicleNorm">
      <key>
        <column name="VehicleId"/>
      </key>
      <one-to-many class="VehicleNorm"/>
    </bag>

  </class>


</hibernate-mapping>

