<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Transport" namespace="Transport.OtherModels.farm">

  <class name="NormType" lazy="false">
    <id name="NormTypeId" >
      <generator class="native"/>
    </id>
    <property name="NormTypeName" length="50" not-null="true"/>
    <property name="ValueType" length="10" not-null="true"/>
    <property name="Coefficient" not-null="true"/>

    <many-to-one name="WorkCounter" column="CounterId" class="WorkCounter" fetch="join" outer-join="true" lazy="false"/>
    
  </class>

</hibernate-mapping>