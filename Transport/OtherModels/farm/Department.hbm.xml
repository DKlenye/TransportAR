<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Transport" namespace="Transport.OtherModels.farm">

  <class name="Department" lazy="false">
    
    <id name="DepartmentId">
      <generator class="native"/>
    </id>       
    
    <property name="DepartmentName" />
    
    <many-to-one name ="Owner" class="TransportOwner" column="OwnerId" not-null="false" lazy="false" />
  
  </class>

</hibernate-mapping>