<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Transport" namespace="Transport.OtherModels.tdbf" xmlns="urn:nhibernate-mapping-2.2">
   <class name="fuelNorm" table="fuelNorms" lazy="false" >
   
     <id name="fuelNormId" column="fuelNormId">
        <generator class="native"/>
     </id>
     
    
    <property name="ownerId">
      <column name="ownerId" sql-type="tinyint" not-null="true" />
    </property>
    
    
    <property name="garageNumber">
      <column name="garageNumber" sql-type="int" not-null="true" />
    </property>


    
    <property name="fuelId">
      <column name="fuelId" sql-type="int" not-null="true" />
    </property>
    

    <property name="basicNorm">
      <column name="basicNorm" sql-type="decimal" not-null="true" />
    </property>
    
    <property name="normType">
      <column name="normType" sql-type="tinyint" not-null="true" />
    </property>
    <property name="notInMetters">
      <column name="notInMetters" sql-type="tinyint" not-null="true" />
    </property>
    <property name="additional">
      <column name="additional" sql-type="tinyint" not-null="true" />
    </property>
    <property name="orderDate">
      <column name="orderDate" sql-type="smalldatetime" not-null="false" />
    </property>
    <property name="orderNumber">
      <column name="orderNumber" sql-type="char" not-null="false" />
    </property>
    <property name="orderNote">
      <column name="orderNote" sql-type="text" not-null="false" />
    </property>
    <property name="note">
      <column name="note" sql-type="varchar" not-null="false" />
    </property>
    <property name="coefficientMh">
      <column name="coefficientMh" sql-type="decimal" not-null="true" />
    </property>

    
    <bag name="fuelNormsexes" inverse="true" cascade="none">
      <key column="fuelNormId" />
      <one-to-many class="fuelNormsEx" />
    </bag>

      <join table="fuelNormsStatus" fetch="join" optional="true" >
         <key>
            <column name="fuelNormId"/>
         </key>
         <property name="enabled"/>
         <property name="startingDate"/>
         <property name="dateOfTerm"/>
      </join>
    
  </class>
</hibernate-mapping>