<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Transport" namespace="Transport.OtherModels.tdbf" xmlns="urn:nhibernate-mapping-2.2">
  <class name="waybillsFuelRemains" table="waybillsFuelRemains" lazy="false" >
  
    <composite-id>
      <key-property name="ownerId" column="ownerId" />
      <key-property name="waybillNumber" column="waybillNumber" />
      <key-property name="garageNumber" column="garageNumber" />
      <key-property name="fuelId" column="fuelId" />
    </composite-id>
    
       
    <property name="departureRemain">
      <column name="departureRemain" sql-type="decimal" not-null="true" />
    </property>
    
    <property name="returnRemain">
      <column name="returnRemain" sql-type="decimal" not-null="true" />
    </property>
    
    <property name="_dbf">
      <column name="_dbf" sql-type="bit" not-null="false" />
    </property>
    
  </class>
</hibernate-mapping>