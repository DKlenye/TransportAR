<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Transport" namespace="Transport.OtherModels.tdbf">

  <class name="BaseVehicle" table="transportFacilities" lazy="false">
    
    <composite-id>
      <key-property name="OwnerId" column="ownerId"/>
      <key-property name="GarageNumber" column="garageNumber"/>
    </composite-id>
    
    <property name="Model" column="model" />
    <property name="RegistrationNumber" column="registrationNumber" />
    <property name="Inventory" column="inventory" />
     <property name="subOwnerId" column="subOwnerId" />

  </class>

</hibernate-mapping>