<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Transport" namespace="Transport.OtherModels.tdbf" xmlns="urn:nhibernate-mapping-2.2">
  <class name="driversToResponse" table="driversToResponse" lazy="false" >
    <composite-id>
      <key-property name="ownerId" column="ownerId" />
      <key-property name="garageNumber" column="garageNumber" />
      <key-property name="driverId" column="driverId" />
    </composite-id>
    
    <property name="ownerId">
      <column name="ownerId" sql-type="tinyint" not-null="true" />
    </property>
    
    
    <property name="garageNumber">
      <column name="garageNumber" sql-type="int" not-null="true" />
    </property>
    
    
    <property name="driverId">
      <column name="driverId" sql-type="int" not-null="true" />
    </property>
    
  </class>
</hibernate-mapping>