<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Transport" namespace="Transport.OtherModels.tdbf">

  <class name="Driver" table="_drivers" lazy="false" where=" personId > 0">

    <id name="DriverId" column="driverId">
      <generator class="native"/>
    </id>

    <property name="OwnerId" column="ownerId"/>
    
    <many-to-one name ="People" class="BasePeople" column="personId"  not-null="true"  fetch="join"  lazy="false" />

  </class>

</hibernate-mapping>