<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Transport" namespace="Transport.OtherModels.tdbf">

  <class name="waybillsTasks"  table="waybillsTasks" lazy="false">


    <id name="waybillTaskId">
      <generator class="native"/>
    </id>
    
    <property name="ownerId"/>
    <property name="garageNumber"/>
    <property name="waybillNumber"/>

    <property name="taskBeginDate" />
    <property name="mh" />
    <property name="km" />
    <property name="mass" />
    <property name="massRace" />
    <property name="trailerGarageNumber" />
    <property name="raceAbroad" />
    <property name="passengers" />
    <property name="accConsumption" />
    <property name="invoiceSetNumber" />
    <property name="raceBY" />
    <property name="isCustomerDebit" />
    <property name="isUnaccounted" />
    <property name="CustomerId" />
    <property name="replicationID" />
    


    <many-to-one name="fuelNorm" class="fuelNorm">
      <column name="fuelNormId"/>
    </many-to-one>




    <bag name="increases" lazy="false" fetch="select">
      <key>
        <column name="waybillTaskId"/>
      </key>
      <one-to-many class="WaybillTaskIncrease"/>
    </bag>
    
    
  </class>

</hibernate-mapping>