<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Transport" namespace="Transport.OtherModels.tdbf" xmlns="urn:nhibernate-mapping-2.2">
  <class name="transportTrailer" table="transportTrailers" lazy="false" >
    <id name="trailerGarageNumber">
      <generator class="identity" />
    </id>
    <property name="model">
      <column name="model" sql-type="varchar" not-null="true" />
    </property>
    <property name="registrationNumber">
      <column name="registrationNumber" sql-type="char" not-null="true" />
    </property>
    <property name="inventory">
      <column name="inventory" sql-type="char" not-null="true" />
    </property>
    <property name="capacityTonns">
      <column name="capacityTonns" sql-type="decimal" not-null="false" />
    </property>
    <property name="dimensionWidth">
      <column name="dimensionWidth" sql-type="decimal" not-null="false" />
    </property>
    <property name="dimensionLength">
      <column name="dimensionLength" sql-type="decimal" not-null="false" />
    </property>
    <property name="dimensionHeight">
      <column name="dimensionHeight" sql-type="decimal" not-null="false" />
    </property>
    <property name="weight">
      <column name="weight" sql-type="decimal" not-null="false" />
    </property>
    <property name="fullWeight">
      <column name="fullWeight" sql-type="decimal" not-null="false" />
    </property>
    <property name="serviceDocNo">
      <column name="serviceDocNo" sql-type="varchar" not-null="false" />
    </property>
    <property name="makeYear">
      <column name="makeYear" sql-type="int" not-null="false" />
    </property>
    <property name="bodyNo">
      <column name="bodyNo" sql-type="varchar" not-null="false" />
    </property>
    
    <property name="bodyTypeId">
      <column name="bodyTypeId" sql-type="int" not-null="false" />
    </property>
    <property name="regSertificate">
      <column name="regSertificate" sql-type="varchar" not-null="false" />
    </property>
    <property name="regfinish">
      <column name="regfinish" sql-type="smalldatetime" not-null="false" />
    </property>
    
    <property name="OwnerId">
      <column name="OwnerId" sql-type="tinyint" not-null="false" />
    </property>

  </class>
</hibernate-mapping>