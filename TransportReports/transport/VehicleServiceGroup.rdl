  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
	ag.AccGroupName,
    ag.CostCode,
    isnull(sg.ServiceGroupName,'НЕТ ГРУППЫ' )ServiceGroupName,
    v.GarageNumber,
    v.RegistrationNumber,
    v.Model
FROM Vehicle v
left JOIN AccGroup ag ON ag.AccGroupId = v.AccGroupId
LEFT JOIN ServiceGroup sg ON sg.ServiceGroupId = v.ServiceGroupId
WHERE v.OwnerId = 1 AND (v.WriteOffDate IS NULL OR v.WriteOffDate&gt;'01.01.2014') and v.columnId &gt;=6</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="AccGroupName">
          <DataField>AccGroupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CostCode">
          <DataField>CostCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServiceGroupName">
          <DataField>ServiceGroupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GarageNumber">
          <DataField>GarageNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RegistrationNumber">
          <DataField>RegistrationNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Model">
          <DataField>Model</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
