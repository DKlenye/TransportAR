    <DataSource Name="DataSource1">
      <DataSourceReference>DB2</DataSourceReference>
      <rd:DataSourceID>72b9ea02-c985-4d76-bd7d-a8aa89b1d947</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="CustomerName">
          <DataField>CustomerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="WaybillCount">
          <DataField>WaybillCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
	CustomerName,
	COUNT(WaybillId) AS WaybillCount
FROM (
SELECT DISTINCT c.CustomerName, w.WaybillId FROM Waybill w
INNER JOIN WaybillTask wt ON wt.WaybillId = w.WaybillId 
INNER JOIN Customer c ON c.CustomerId = wt.CustomerId
WHERE CAST(w.DepartureDate AS Date) BETWEEN @start AND @end AND  w.WaybillState&gt;1
)a
GROUP BY CustomerName
ORDER BY 1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@start">
            <Value>=Parameters!start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end">
            <Value>=Parameters!end.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
