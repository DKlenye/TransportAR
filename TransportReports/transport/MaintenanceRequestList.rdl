      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	mr.*,
	v.model,
	v.registrationNumber,
	v.garageNumber,
	d.DepartmentName
from MaintenanceRequest mr
left join Vehicle v on v.VehicleId = mr.VehicleId
left join Department d on d.DepartmentId = @DepartmentId
where mr.DepartmentId = @DepartmentId
order by mr.MaintenanceRequestId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DepartmentId">
            <Value>=Parameters!DepartmentId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
