  <Code>Function ToTime(i As Integer) As String
	
If i = 0 Then
		Return ""
	End If

	Dim hours = Int((i) / 60)
	Dim minutes = Int(i Mod 60)
	Return System.[String].Format("{0}:{1}", hours, minutes)
End Function</Code>
